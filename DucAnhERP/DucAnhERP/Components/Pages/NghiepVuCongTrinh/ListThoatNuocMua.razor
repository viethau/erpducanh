                { "Thông tin chung hố ga Chát Mặt Trong", Input.ThongTinChungHoGa_ChatMatTrong ?? string.Empty },
                { "Thông tin chung hố ga Chát Mặt Ngoài", Input.ThongTinChungHoGa_ChatMatNgoai ?? string.Empty },
                { "Thông tin tấm đan hố ga Hình Thức Đậy Hố Ga", Input.ThongTinTamDanHoGa2_HinhThucDayHoGa ?? string.Empty },
                { "Thông tin vật liệu đào hố ga Loại Vật Liệu Đào", Input.ThongTinVatLieuDaoHoGa_LoaiVatLieuDao ?? string.Empty },
                { "Thông tin đường truyền dẫn Hình Thức Truyền Dẫn", Input.ThongTinDuongTruyenDan_HinhThucTruyenDan ?? string.Empty },
                { "Thông tin đường truyền dẫn Loại Truyền Dẫn", Input.ThongTinDuongTruyenDan_LoaiTruyenDan ?? string.Empty },
                { "Thông tin móng đường truyền dẫn Loại Móng", Input.ThongTinMongDuongTruyenDan_LoaiMong ?? string.Empty },
                { "Thông tin móng đường truyền dẫn Cấu Tạo Đê Cống", Input.ThongTinDeCong_CauTaoDeCong ?? string.Empty },
                { "Thông tin kích thước hình học cống hộp, rãnh Cấu Tạo Tường", Input.TTKTHHCongHopRanh_CauTaoTuong ?? string.Empty },
                { "Thông tin kích thước hình học cống hộp, rãnh Cấu Tạo Mũ Mố", Input.TTKTHHCongHopRanh_CauTaoMuMo ?? string.Empty },
                { "Thông tin kích thước hình học cống hộp, rãnh Chất Mặt Trong", Input.TTKTHHCongHopRanh_ChatMatTrong ?? string.Empty },
                { "Thông tin kích thước hình học cống hộp, rãnh Chất Mặt Ngoài", Input.TTKTHHCongHopRanh_ChatMatNgoai ?? string.Empty },
                { "Thanh chống Cấu Tạo Thanh Chống", Input.TTKTHHCongHopRanh_CauTaoThanhChong ?? string.Empty },
                { "Thông tin tấm đan tiêu chuẩn Cấu Tạo Tấm Dẫn Truyền Dẫn Tấm Dẫn Tiêu Chuẩn", Input.TTTDCongHoRanh_CauTaoTamDanTruyenDanTamDanTieuChuan ?? string.Empty },
                { "Thông tin tấm đan loại 02 Cấu Tạo Tấm Dẫn Truyền Dẫn", Input.TTTDCongHoRanh_CauTaoTamDanTruyenDan ?? string.Empty },
                { "Thông tin vật liệu đào cống, rãnh Loại Vật Liệu Đào", Input.TTVLDCongRanh_LoaiVatLieuDao ?? string.Empty }
            };

            // Kiểm tra các biến
            foreach (var check in checks)
            {
                if (!string.IsNullOrEmpty(check.Value) && !listDanhMuc.Any(d => d.Id == check.Value))
                {
                    result = false;
                    message = $"Vui lòng kiểm tra {check.Key}!";
                    break;
                }
            }

            if (!result)
            {
                // Hiển thị thông báo nếu không tồn tại danh mục
                ToastService.ShowToast(
                    "Thông báo",
                    message,
                    cssClass: "toast-success",
                    iconClass: "bi bi-info-circle",
                    progressBarClass: "bg-info",
                    duration: 5000
                );
            }

            return result;
        }
        catch (Exception ex)
        {
            Console.WriteLine(ex.Message);
            throw;
        }
