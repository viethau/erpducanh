    // Truy vấn data theo điều kiện tìm kiếm
    private async Task LoadData()
    {
        var helper = new DataConversionHelper();

        listDanhMuc = await DanhMucRepository.GetAll();
        var hopranhthangs = await HopRanhThangRepository.GetData();
        var convertDatas = await helper.ConvertDataHopRanhThang(hopranhthangs, listDanhMuc);
        listModel = convertDatas;
        listHopRanhThang = Paging(convertDatas);

    }

    public string GetTenDanhMucById(string id)
    {
        var danhMuc = listDanhMuc.FirstOrDefault(dm => dm.Id == id);
        return danhMuc != null ? danhMuc.Ten : null;
    }



    //modal
    private bool isModalVisible = false;
    private string TitleModal = "Thêm mới thông tin";
    private void ShowModal()
    {
        isModalVisible = true;
        Input = new MHopRanhThang();
    }

    private void HandleModalClose(bool isVisible)
    {
        isModalVisible = isVisible;
    }

    private void HandleSave()
    {
        // Logic for saving
        isModalVisible = false;
    }

    private void HandleCancel()
    {
        // Logic for canceling
        isModalVisible = false;
        Input = new();
    }


    //Thêm sửa
    private bool isEdit = false;
    private async Task HandleEdit(string Id)
    {
        isEdit = true;
        Input = await HopRanhThangRepository.GetById(Id);
        TitleModal = "Cập nhật thông tin";
        isModalVisible = true;
    }

    private async Task HandleValidSubmit()
    {
        Console.WriteLine(Input);

        // Kiểm tra xem danh mục có tồn tại hay không
        bool check = await CheckIsExitsIdDanhMuc();

        if (check)
        {
            if (isEdit)
            {
                // Cập nhật thông tin
                await HopRanhThangRepository.Update(Input);
                isModalVisible = false;
                isEdit = false;
                Input = new MHopRanhThang();
                ToastService.ShowToast(
                    "Thông báo",
                    "Cập nhật thành công.",
                    cssClass: "toast-success",
                    iconClass: "fa fa-bell",
                    progressBarClass: "bg-info",
                    duration: 5000
                );
                await LoadData();
            }
            else
            {
                // Thêm mới thông tin
                await HopRanhThangRepository.Insert(Input);
                isModalVisible = false;
                Input = new MHopRanhThang();
                ToastService.ShowToast(
                    "Thông báo",
                    "Thêm thành công.",
                    cssClass: "toast-success",
                    iconClass: "fa fa-bell",
                    progressBarClass: "bg-info",
                    duration: 5000
                );
                await LoadData();
            }
        }

    }

    public async Task<bool> CheckIsExitsIdDanhMuc()
    {
        try
        {
            bool result = true;
            string message = "";

            // Lấy danh sách danh mục
            listDanhMuc = await DanhMucRepository.GetAll();

            // Tạo danh sách các biến cần kiểm tra
            var checks = new Dictionary<string, string>
            {
                { "Thông tin chung hố ga Hình Thức Hố Ga", Input.ThongTinChungHoGa_HinhThucHoGa ?? string.Empty },
                { "Thông tin chung hố ga Kết Cấu Mũ Mố", Input.ThongTinChungHoGa_KetCauMuMo ?? string.Empty },
                { "Thông tin chung hố ga Kết Cấu Tường", Input.ThongTinChungHoGa_KetCauTuong ?? string.Empty },
                { "Thông tin chung hố ga Hình Thức Móng Hố Ga", Input.ThongTinChungHoGa_HinhThucMongHoGa ?? string.Empty },
